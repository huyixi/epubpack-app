#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huyixi/i/epubpack-app/node_modules/.pnpm/electron@30.5.1/node_modules/electron/node_modules:/Users/huyixi/i/epubpack-app/node_modules/.pnpm/electron@30.5.1/node_modules:/Users/huyixi/i/epubpack-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/huyixi/i/epubpack-app/node_modules/.pnpm/electron@30.5.1/node_modules/electron/node_modules:/Users/huyixi/i/epubpack-app/node_modules/.pnpm/electron@30.5.1/node_modules:/Users/huyixi/i/epubpack-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/electron@30.5.1/node_modules/electron/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/electron@30.5.1/node_modules/electron/cli.js" "$@"
fi
